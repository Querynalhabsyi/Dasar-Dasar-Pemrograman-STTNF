grade = int(input("Masukkan nilai Anda\t: "))

if(grade >= 90 and grade <= 100):
    print("Grade\t\t: A")
elif(grade >= 80 and grade < 90):
    print("Grade\t\t: B+")
elif(grade >= 70 and grade < 80):
    print("Grade\t\t: B")
elif(grade >= 60 and grade < 70):
    print("Grade\t\t: C+")
elif(grade >= 50 and grade < 60):
    print("Grade\t\t: C")
elif(grade >= 40 and grade < 50):
    print("Grade\t\t: D")
else:
    print("Kamu belum lulus, silakan remedial")
